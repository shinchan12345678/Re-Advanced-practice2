<div class="container">
  <div class="row mb-4">
    <div class="col-md-3"></div>
    <div class="col-md-8 offset-md-1">
      <%= form_with url: searches_path ,method: :get do |f| %>
        <%= f.text_field :query %>
        <%= f.select :model,["User","Book"] %>
        <%= f.select :condition,["部分一致","前方一致","後方一致","完全一致"] %>
        <%= f.submit "Serach" %>
      <% end %>
    </div>
  </div>
</div>