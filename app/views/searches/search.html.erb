<div class="container">
  <div class="row">
    <div class="col-12">
      <% if @model=="User" %>
        <h2>Users search for "<%= @query %>"</h2>
        <%= render 'users/index',users: @users %>
      <% else %>
        <h2>Books search for "<%= @query %>"</h2>
        <%= render 'books/index',books: @books %>
      <% end %>
    </div>
  </div>
</div>
